{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@public/*": [
        "../public/*"
      ],
      "@content/*": [
        "content/*"
      ],

    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  }
}